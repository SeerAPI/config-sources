		<OldEffect Id="822" />                       <!-- effectIcon.xml id -->
		<AdvEffect Id="822" Des="this is &lt;font color='#FF7751'&gt;new&lt;/font&gt; Icon html text" />	 <!-- effectIcon-id   描述 htmltext   &lt;font color='#FF7751'&gt;  word   &lt;/font&gt;  -->
